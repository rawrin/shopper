{
  "name": "shopper_challenge",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "watch": "npm run watchify & npm run less:watch & npm run start",
    "build": "npm run build:js && npm run less:dist",
    "prebuild:js": "mkdirp ./public/js",
    "prewatchify": "npm run prebuild:js",
    "watchify": "watchify -t babelify ./src/main/app.js -o ./public/js/bundle.js --debug -v",
    "build:js": "browserify -t babelify .src//main/app.js -o ./public/js/bundle.js",
    "less": "lessc --source-map-map-inline --npm-import --autoprefix ./src/main/app.less ./public/css/bundle.css",
    "less:watch": "watch 'npm run less' ./src/main --filter='./lessFilter.js'",
    "less:dist": "lessc --compress --npm-import --autoprefix ./src/main/app.less ./public/css/bundle.css",
    "start": "nodemon ./bin/www",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "babel-register": "6.5.2",
    "body-parser": "~1.13.2",
    "cookie-parser": "~1.3.5",
    "debug": "~2.2.0",
    "express": "~4.13.1",
    "express-session": "1.13.0",
    "jade": "~1.11.0",
    "morgan": "~1.6.1",
    "react": "0.14.7",
    "react-dom": "0.14.7",
    "react-loader": "2.0.0",
    "react-select": "0.9.1",
    "serve-favicon": "~2.3.0",
    "sqlite3": "3.1.1",
    "superagent": "1.7.2"
  },
  "devDependencies": {
    "babel-plugin-transform-decorators": "6.5.0",
    "babel-plugin-transform-react-jsx": "6.5.0",
    "babel-preset-es2015": "6.5.0",
    "babelify": "7.2.0",
    "browserify": "13.0.0",
    "less": "2.6.0",
    "less-plugin-autoprefix": "1.5.1",
    "less-plugin-npm-import": "2.1.0",
    "mkdirp": "0.5.1",
    "watch": "0.17.1",
    "watchify": "3.7.0"
  }
}
